<testCase name="testDateVariableYear">
   <testInstance formula="DATE(year__c,2,29)" returntype="DateOnly">
    <SqlOutput nullAsNull="true">
       <Sql>CAST(from_iso8601_date(CONCAT(CAST(CAST(FLOOR(COALESCE($!s0s!$.year__c, 0)) AS VARCHAR) AS VARCHAR),'-',CAST(2 AS VARCHAR),'-',CAST(29 AS VARCHAR))) AS TIMESTAMP)</Sql>
       <Guard>COALESCE($!s0s!$.year__c, 0) is null OR COALESCE($!s0s!$.year__c, 0)&lt; 1 OR COALESCE($!s0s!$.year__c, 0)&gt; 9999 OR  29 &gt;= day(LAST_DAY_OF_MONTH(DATE(CONCAT(CAST(FLOOR(COALESCE($!s0s!$.year__c, 0)) AS VARCHAR),'-',CAST(2 AS VARCHAR),'-01'))))+1 </Guard>
    </SqlOutput>
    <SqlOutput nullAsNull="false">
       <Sql>CASE WHEN $!s0s!$.year__c IS NULL THEN NULL ELSE CAST(from_iso8601_date(CONCAT(CAST(CAST(FLOOR($!s0s!$.year__c) AS VARCHAR) AS VARCHAR),'-',CAST(2 AS VARCHAR),'-',CAST(29 AS VARCHAR))) AS TIMESTAMP) END</Sql>
       <Guard>NOT ($!s0s!$.year__c IS NULL ) AND ($!s0s!$.year__c is null OR $!s0s!$.year__c&lt; 1 OR $!s0s!$.year__c&gt; 9999 OR  29 &gt;= day(LAST_DAY_OF_MONTH(DATE(CONCAT(CAST(FLOOR($!s0s!$.year__c) AS VARCHAR),'-',CAST(2 AS VARCHAR),'-01'))))+1 )</Guard>
    </SqlOutput>
