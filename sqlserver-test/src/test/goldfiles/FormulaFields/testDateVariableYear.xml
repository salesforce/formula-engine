<testCase name="testDateVariableYear">
   <testInstance formula="DATE(year__c,2,29)" returntype="DateOnly">
    <SqlOutput nullAsNull="true">
       <Sql>DATEFROMPARTS(CAST(FLOOR(COALESCE($!s0s!$.year__c, 0)) AS VARCHAR),2,29)</Sql>
       <Guard>COALESCE($!s0s!$.year__c, 0) is null OR COALESCE($!s0s!$.year__c, 0)&lt; 1 OR COALESCE($!s0s!$.year__c, 0)&gt; 9999 OR  29 &gt;= DAY(EOMONTH(TO_DATE(FLOOR(COALESCE($!s0s!$.year__c, 0)) || '-' || 2,'YYYY-MM')))+1 </Guard>
    </SqlOutput>
    <SqlOutput nullAsNull="false">
       <Sql>CASE WHEN $!s0s!$.year__c IS NULL THEN NULL ELSE DATEFROMPARTS(CAST(FLOOR($!s0s!$.year__c) AS VARCHAR),2,29) END</Sql>
       <Guard>NOT ($!s0s!$.year__c IS NULL ) AND ($!s0s!$.year__c is null OR $!s0s!$.year__c&lt; 1 OR $!s0s!$.year__c&gt; 9999 OR  29 &gt;= DAY(EOMONTH(TO_DATE(FLOOR($!s0s!$.year__c) || '-' || 2,'YYYY-MM')))+1 )</Guard>
    </SqlOutput>
    <JsOutput highPrec="true" nullAsNull="false">new Date(Date.UTC($F.nvl(context.record.year__c,new $F.Decimal('0')).toNumber(),new $F.Decimal('2').toNumber()-1,new $F.Decimal('29').toNumber()))</JsOutput>
