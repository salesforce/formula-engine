       <Sql>'2013-12-31 01:02:03Z'</Sql>
       <Guard>null</Guard>
    </SqlOutput>
      <result>
      <inputvalues>No data</inputvalues>
         <formula>2013-12-31 01:02:03Z</formula>
         <sql>2013-12-31 01:02:03Z</sql>
         <formulaNullAsNull>2013-12-31 01:02:03Z</formulaNullAsNull>
         <sqlNullAsNull>2013-12-31 01:02:03Z</sqlNullAsNull>
      </result>
   </testInstance>
</testCase>
