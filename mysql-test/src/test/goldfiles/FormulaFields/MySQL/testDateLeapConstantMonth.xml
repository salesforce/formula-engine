<?xml version="1.0" encoding="UTF-8"?>
<testCase name="testDateLeapConstantMonth">
   <testInstance formula="DATE(year__c,2,day__c)" returntype="DateOnly">
    <SqlOutput nullAsNull="true">
       <Sql>DATE(CONCAT(CONVERT(FLOOR(COALESCE($!s0s!$.year__c, 0)),CHAR),'-',2,'-',CONVERT(FLOOR(COALESCE($!s0s!$.day__c, 0)),CHAR)))</Sql>
       <Guard>COALESCE($!s0s!$.year__c, 0) is null OR COALESCE($!s0s!$.year__c, 0)&lt; 1 OR COALESCE($!s0s!$.year__c, 0)&gt; 9999 OR COALESCE($!s0s!$.day__c, 0) is null OR COALESCE($!s0s!$.day__c, 0)&lt; 1 OR COALESCE($!s0s!$.day__c, 0) &gt;= 32 OR  COALESCE($!s0s!$.day__c, 0) &gt;= LAST_DAY(DATE(CONCAT(FLOOR(COALESCE($!s0s!$.year__c, 0)),'-',2,'-01')))+1 </Guard>
    </SqlOutput>
    <SqlOutput nullAsNull="false">
       <Sql>CASE WHEN $!s0s!$.year__c IS NULL THEN NULL WHEN $!s0s!$.day__c IS NULL THEN NULL ELSE DATE(CONCAT(CONVERT(FLOOR($!s0s!$.year__c),CHAR),'-',2,'-',CONVERT(FLOOR($!s0s!$.day__c),CHAR))) END</Sql>
       <Guard>NOT ($!s0s!$.year__c IS NULL OR $!s0s!$.day__c IS NULL ) AND ($!s0s!$.year__c is null OR $!s0s!$.year__c&lt; 1 OR $!s0s!$.year__c&gt; 9999 OR $!s0s!$.day__c is null OR $!s0s!$.day__c&lt; 1 OR $!s0s!$.day__c &gt;= 32 OR  $!s0s!$.day__c &gt;= LAST_DAY(DATE(CONCAT(FLOOR($!s0s!$.year__c),'-',2,'-01')))+1 )</Guard>
    </SqlOutput>
      <result>
      <inputvalues>[2001, 29]</inputvalues>
         <formula>Error: com.force.formula.FormulaEvaluationException</formula>
         <sql>null</sql>
         <formulaNullAsNull>Error: com.force.formula.FormulaEvaluationException</formulaNullAsNull>
         <sqlNullAsNull>null</sqlNullAsNull>
      </result>
      <result>
      <inputvalues>[2001, 28]</inputvalues>
         <formula>Wed Feb 28 00:00:00 GMT 2001</formula>
         <sql>2001-02-28 00:00:00.0</sql>
