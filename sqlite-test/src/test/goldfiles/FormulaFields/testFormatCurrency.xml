<?xml version="1.0" encoding="UTF-8"?>
<testCase name="testFormatCurrency">
   <testInstance formula="FORMATCURRENCY(isocode__c, customNumber1__c)" returntype="Text">
    <SqlOutput nullAsNull="true">
       <Sql>COALESCE($!s0s!$.isocode__c,'')||' '||FORMAT(CASE WHEN $!s0s!$.isocode__c IN('JPY')THEN&quot;%,.0f&quot;WHEN $!s0s!$.isocode__c IN('KWD')THEN&quot;%,.3f&quot;ELSE&quot;%,.2f&quot;END,COALESCE($!s0s!$.customNumber1__c, 0))</Sql>
       <Guard>null</Guard>
    </SqlOutput>
    <SqlOutput nullAsNull="false">
       <Sql>CASE WHEN $!s0s!$.customNumber1__c IS NULL THEN NULL ELSE COALESCE($!s0s!$.isocode__c,'')||' '||FORMAT(CASE WHEN $!s0s!$.isocode__c IN('JPY')THEN&quot;%,.0f&quot;WHEN $!s0s!$.isocode__c IN('KWD')THEN&quot;%,.3f&quot;ELSE&quot;%,.2f&quot;END,$!s0s!$.customNumber1__c)END</Sql>
       <Guard>null</Guard>
    </SqlOutput>
      <result>
