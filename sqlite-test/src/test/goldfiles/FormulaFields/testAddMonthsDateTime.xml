<testCase name="testAddMonthsDateTime">
   <testInstance formula="ADDMONTHS(customdatetime1__c,customnumber1__c)" returntype="DateTime">
    <SqlOutput nullAsNull="true">
       <Sql>datetime($!s0s!$.customdatetime1__c,  (CASE WHEN strftime('%d', $!s0s!$.customdatetime1__c, 'start of month', '+1 month', '-1 day') = strftime('%d', $!s0s!$.customdatetime1__c) THEN '1' ELSE '0' END) || ' day', CASE WHEN (COALESCE($!s0s!$.customnumber1__c, 0)) &gt; 0 THEN FLOOR(COALESCE($!s0s!$.customnumber1__c, 0)) ELSE CEIL(COALESCE($!s0s!$.customnumber1__c, 0)) END || ' month', '-' ||  (CASE WHEN strftime('%d', $!s0s!$.customdatetime1__c, 'start of month', '+1 month', '-1 day') = strftime('%d', $!s0s!$.customdatetime1__c) THEN '1' ELSE '0' END) || ' day')</Sql>
       <Guard>null</Guard>
    </SqlOutput>
