   <testInstance formula="MOD(FLOOR(customnumber1__c),ABS(customnumber2__c))" returntype="Double" precision="12" scale="2">
    <SqlOutput nullAsNull="true">
       <Sql>CASE WHEN SIGN(ABS(COALESCE($!s0s!$.customnumber2__c, 0))) = SIGN(CASE WHEN COALESCE($!s0s!$.customnumber1__c, 0)&gt;=0 THEN FLOOR(COALESCE($!s0s!$.customnumber1__c, 0)) ELSE CEIL(COALESCE($!s0s!$.customnumber1__c, 0)) END) THEN (CASE WHEN COALESCE($!s0s!$.customnumber1__c, 0)&gt;=0 THEN FLOOR(COALESCE($!s0s!$.customnumber1__c, 0)) ELSE CEIL(COALESCE($!s0s!$.customnumber1__c, 0)) END  - (FLOOR(CASE WHEN COALESCE($!s0s!$.customnumber1__c, 0)&gt;=0 THEN FLOOR(COALESCE($!s0s!$.customnumber1__c, 0)) ELSE CEIL(COALESCE($!s0s!$.customnumber1__c, 0)) END/ABS(COALESCE($!s0s!$.customnumber2__c, 0))) * ABS(COALESCE($!s0s!$.customnumber2__c, 0)))) ELSE (CASE WHEN COALESCE($!s0s!$.customnumber1__c, 0)&gt;=0 THEN FLOOR(COALESCE($!s0s!$.customnumber1__c, 0)) ELSE CEIL(COALESCE($!s0s!$.customnumber1__c, 0)) END  - (CEIL(CASE WHEN COALESCE($!s0s!$.customnumber1__c, 0)&gt;=0 THEN FLOOR(COALESCE($!s0s!$.customnumber1__c, 0)) ELSE CEIL(COALESCE($!s0s!$.customnumber1__c, 0)) END/ABS(COALESCE($!s0s!$.customnumber2__c, 0))) * ABS(COALESCE($!s0s!$.customnumber2__c, 0)))) END </Sql>
       <Guard>null</Guard>
    </SqlOutput>
